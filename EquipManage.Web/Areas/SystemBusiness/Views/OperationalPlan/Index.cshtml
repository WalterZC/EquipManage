@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_List.cshtml";
}
<style type="text/css">
    .hiddenCol {
        display: none;
    }

    table .form-control {
        border: 0px solid #c2cad8;
    }

    /*table.dataTable tbody td {
        padding: 3px;
    }*/

    .table-td-vertical-align {
        vertical-align: middle !important;
    }

    .form-group {
        margin-bottom: 0px;
    }
</style>
<div class="page-content">

    <!-- BEGIN EXAMPLE TABLE PORTLET-->
    <div class="portlet light bordered">
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-file-text-o"></i>
                <span class="caption-subject">作业计划列表</span>
            </div>
            <div class="actions">
                <a data-toggle="modal" href="#DailyReportSearch" authorize="yes" class="btn btn-default btn-sm" id="EM-parts">
                    <i class="fa fa-search"></i> 筛选
                </a>
                <a href="javascript:;" class="btn  btn-default btn-sm" authorize="yes" id="EM-detail">
                    <i class="fa fa-file-text-o"></i> 查看
                </a>
                <a href="javascript:;" class="btn  btn-default btn-sm" authorize="yes" id="EM-edit">
                    <i class="fa fa-edit"></i> 编辑
                </a>
                <a href="javascript:;" class="btn  btn-default btn-sm" authorize="yes" id="EM-delete">
                    <i class="fa fa-remove"></i> 删除
                </a>
                <a href="javascript:;" class="btn  btn-default btn-sm" authorize="yes" id="EM-check">
                    <i class="fa fa-check-square-o"></i> 审核
                </a>
            </div>
        </div>
        <div class="portlet-body table-both-scroll">
            <table class="table table-striped table-bordered table-advance table-condensed order-column display nowrap" id="DataList">
                <thead>
                    <tr>
                        <th>内码</th>
                        <th>计划编号</th>
                        <th>计划名称</th>
                        <th>组织部门</th>
                        <th>作业类型</th>
                        <th>作业级别</th>
                        <th>时间单位</th>
                        <th>开始日期</th>
                        <th>截至日期</th>
                        <th>日期间隔</th>
                        <th>有效</th>
                        <th>制作日期</th>
                        <th>制作人</th>
                        <th>审核日期</th>
                        <th>审核人</th>
                        <th>状态</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>


<script src="~/Areas/SystemBusiness/Scripts/OperationalPlan/Index.js"></script>
